{
  "name": "Brush-Flow",
  "version": "1.8",
  "description": "A survey tool for answer question autorize",
  "permissions": [
    "contextMenus",
    "proxy",
    "tabs",
    "activeTab",
    "declarativeContent",
    "storage",
    "webRequest",
    "browsingData",
    "webRequestBlocking",
    "downloads",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "scripts": [
      "library/js/jquery.min.js",
      "background/helper.js",
      "background/background.js"
    ],
    "persistent": true
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": [
        "library/js/jquery.min.js",
        "front/helper.js"
      ],
      "all_frames": true,
      "run_at": "document_start"
    },
    {
      "matches": ["*://hao.7654.com/*"],
      "js": [
        "front/7654.js"
      ],
      "all_frames": true,
      "run_at": "document_end"
    },
    {
      "matches": ["*://123.sogou.com/*"],
      "js": [
        "front/sougou.js"
      ],
      "all_frames": true,
      "run_at": "document_end"
    },
    {
      "matches": ["*://www.hao123.com/?tn*"],
      "js": [
        "front/hao123.js"
      ],
      "all_frames": true,
      "run_at": "document_end"
    }
  ],
  "browser_action": {
    "default_popup": "popup/popup.html",
    "default_icon": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    }
  },
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  },
  "manifest_version": 2
}